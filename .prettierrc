{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "importOrder": [
    "^(next/(.*)$)|^(next$)",
    "^(react/(.*)$)|^(react$)",
    "^(@nextui-org/(.*)$)|^(@nextui-org$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^~/app/(.*)$",
    "^~/components/(.*)$",
    "^~/features/(.*)$",
    "^~/layouts/(.*)$",
    "^~/lib/(.*)$",
    "^~/utils/(.*)$",
    "",
    "^~/assets/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
